#.bash_aliases_cygwin

# FS clipboard support
alias putclip="cat > /dev/clipboard"
alias getclip="cat /dev/clipboard"
alias pbcopy="putclip"
alias pbpaste="getclip"
alias open="cygstart"
alias start="cygstart"


# Vim
alias gvim="C:/Program\ Files\ \(x86\)/Vim/vim74/gvim.exe"

# Ensure ls uses colours
alias ls="ls --color=auto"
